{
  "version": 2,
  "name": "ai-nudge-backend",
  "builds": [
    {
      "src": "api/vercel_handler.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/vercel_handler.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "ENVIRONMENT": "production"
  },
  "functions": {
    "api/vercel_handler.py": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"]
}
