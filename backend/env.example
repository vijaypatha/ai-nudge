# AI Nudge Backend Environment Variables
# Copy this file to .env and fill in your actual values

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/ai_nudge_db

# Redis Configuration
REDIS_URL=redis://localhost:6379

# JWT Authentication
JWT_SECRET_KEY=your-super-secret-jwt-key-change-this-in-production

# External API Keys
OPENAI_API_KEY=your-openai-api-key-here
TWILIO_ACCOUNT_SID=your-twilio-account-sid-here
TWILIO_AUTH_TOKEN=your-twilio-auth-token-here

# OAuth Configuration
GOOGLE_CLIENT_ID=your-google-client-id-here
GOOGLE_CLIENT_SECRET=your-google-client-secret-here

# Application Configuration
ENVIRONMENT=development
PYTHONPATH=.

# WebSocket Configuration
WEBSOCKET_ALLOWED_ORIGINS=["http://localhost:3000"]

# Optional: Logging
LOG_LEVEL=INFO

# Optional: Feature Flags
ENABLE_MLS_INTEGRATION=true
ENABLE_TWILIO_INTEGRATION=true
ENABLE_GOOGLE_OAUTH=true 